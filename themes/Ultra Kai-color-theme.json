{
	"name": "Ultra Kai",
	"type": "dark",
	"colors": {
		"editor.background": "#1d1d1d",
		"editor.foreground": "#e0e0e0",
		"activityBarBadge.background": "#ff0000",
		"sideBarTitle.foreground": "#bbbbbb"
